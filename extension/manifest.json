{
  "name": "GX_app",  
  "version": "1.0",  
  "icons": {"128": "logo.ico"},  
  "description": "Get title and url of web page that you are opening now", 
  "browser_action": {
    "default_title": "GX_app",
    "default_popup": "index.html"  
  },
  "content_scripts": [ 
    { 
      "js": [ "jquery.min.js", "script.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
    }
  ],
  "permissions": [
    "activeTab",
    "http://*/"
  ],  
  "author": "ywzx",  
  "manifest_version": 2  
}