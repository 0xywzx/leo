{
  "name": "GX_app",  
  "version": "1.0",  
  "icons": {"128": "images/logo.ico"},  
  "description": "Get title and url of web page that you are opening now", 
  "author": "ywzx",  
  "manifest_version": 2,  
  "browser_action": {
    "default_title": "GX_app",
    "default_popup": "index.html"  
  },
  "content_scripts": [ 
    { 
      "matches": [ "<all_urls>" ],
      "js": [ "content_script.js" ]
    }
  ],
  "permissions": [
    "storage", "activeTab"
  ]
}
